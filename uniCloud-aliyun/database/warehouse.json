// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
// 转运仓信息表
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": true,
		"create": true,
		"update": true,
		"delete": true
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"company": {
			"bsonType":"string",
			"description":"物流公司名称",
			"defaultValue":""
		},
		"city": {
			"bsonType":"string",
			"description":"仓库所在城市",
			"defaultValue":""
		},
		"shippingWay": {
			"bsonType":"string",
			"description":"物流方式  sea海运 car陆运",
			"defaultValue":""
		},
		"price": {
			"bsonType":"string",
			"description":"物流价格",
			"defaultValue":""
		},
		"shippingCode": {
			"bsonType":"string",
			"description":"仓库代码",
			"defaultValue":""
		},
		"userId": {
			"bsonType":"string",
			"description":"客户uid",
			"foreignKey":"uni-id-users._id" // 关联用户表_id
		}
	}
}